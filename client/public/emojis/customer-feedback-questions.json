{
  "questionSets": {
    "banking-customer-feedback": {
      "name": "Banking Customer Feedback Survey",
      "description": "Complete customer feedback survey with satisfaction, likelihood, and ease questions",
      "questions": [
        {
          "id": "satisfaction-experience",
          "type": "emoji_scale",
          "title": "How satisfied are you with the experience you received at Platinum Banking?",
          "required": true,
          "emojiSet": "customer-satisfaction",
          "scale": "5-point",
          "settings": {
            "allowComments": true,
            "showLabels": true,
            "showValues": true
          }
        },
        {
          "id": "likelihood-recommend",
          "type": "emoji_scale",
          "title": "Based on your experience at Platinum Banking, how likely are you to recommend us to your friends and colleagues?",
          "required": true,
          "emojiSet": "likelihood-recommendation",
          "scale": "10-point",
          "settings": {
            "allowComments": false,
            "showLabels": true,
            "showValues": true,
            "rangeLabels": {
              "1-6": "Unlikely",
              "7-8": "Neutral", 
              "9-10": "Likely"
            }
          }
        },
        {
          "id": "ease-interaction",
          "type": "emoji_scale",
          "title": "How easy was it to interact with us?",
          "required": true,
          "emojiSet": "ease-of-interaction",
          "scale": "5-point",
          "settings": {
            "allowComments": true,
            "showLabels": true,
            "showValues": true
          }
        },
        {
          "id": "comments",
          "type": "text",
          "title": "We would love to hear from you, please provide your comments. (Optional)",
          "required": false,
          "settings": {
            "multiline": true,
            "maxLength": 1000,
            "placeholder": "We would love to hear from you, please provide your comments. (Optional)"
          }
        },
        {
          "id": "phone-number",
          "type": "text",
          "title": "Phone Number (Optional)",
          "required": false,
          "settings": {
            "inputType": "tel",
            "placeholder": "+233",
            "showCountryFlag": true,
            "countryCode": "GH"
          }
        }
      ]
    },
